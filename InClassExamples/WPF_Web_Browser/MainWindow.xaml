<Window x:Class="WPF_Web_Browser.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPF_Web_Browser"
        mc:Ignorable="d"
        Title="Let Me Google That For You" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="txtQuery" HorizontalAlignment="Left" Height="46" Margin="170,68,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="230"/>
        <Button x:Name="btnGo" Content="GO" HorizontalAlignment="Left" Height="30" Margin="436,76,0,0" VerticalAlignment="Top" Width="65" Click="btnGo_Click"/>

    </Grid>
</Window>
